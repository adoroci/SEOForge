{
    "name": "probeforge/seoforge",
    "description": "Comprehensive SEO audit and automated fixing tool for Laravel 12 Blade templates. Analyze and optimize your application's SEO compliance with A/AA/AAA standards.",
    "keywords": [
        "laravel",
        "seo",
        "audit",
        "optimization",
        "blade",
        "templates",
        "meta-tags",
        "open-graph",
        "twitter-cards",
        "schema-org",
        "structured-data"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adnan Doroci",
            "email": "hi@probeforge.com",
            "homepage": "https://probeforge.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "symfony/dom-crawler": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "ProbeForge\\SEOForge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProbeForge\\SEOForge\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProbeForge\\SEOForge\\SeoForgeServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
} 