{
    "name": "probeforge/seoforge",
    "description": "Early-stage SEO troubleshooting tool for Laravel 12 Blade templates. Detects missing SEO elements and can fix basic structural issues - content still needs manual work.",
    "keywords": [
        "laravel",
        "seo",
        "audit",
        "troubleshooting",
        "blade",
        "templates",
        "alpha",
        "development"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adnan Doroci",
            "email": "hi@probeforge.com",
            "homepage": "https://probeforge.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "symfony/dom-crawler": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "ProbeForge\\SEOForge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProbeForge\\SEOForge\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProbeForge\\SEOForge\\SeoForgeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
} 